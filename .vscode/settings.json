{
  "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.github": true,
      "**/.gitp*": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/.vscode": true,
      "**/core.Microsoft*": true,
      "**/core.mongo*": true,
      "**/core.python*": true,
      "**/CVS": true
   },
   "files.autoSave": "off",
   "workbench.colorTheme": "JellyFish",
   "terminal.integrated.gpuAcceleration": "canvas",
   "cSpell.words": [
      "cachetools",
      "certifi",
      "cffi",
      "collumns",
      "convertedinto",
      "CREDS",
      "gspread",
      "idna",
      "pyasn",
      "pycache",
      "pycparser",
      "surplis",
      "urllib",
      "venv"
   ],
}
